<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Peer Conradi</Authors>
    <Description>A library that contains basic logic and object Models for Model United Nations Conferences and used within the MUNity Core and MUNity Frontend. You can use this base to create custom MUN Apps or clients.</Description>
    <Copyright>Copyright 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/PeerConradi/munityData</RepositoryUrl>
    <PackageReleaseNotes>Updated Hub to only need Petition</PackageReleaseNotes>
    <AssemblyVersion>1.0.15.0</AssemblyVersion>
    <FileVersion>1.0.15.0</FileVersion>
    <Version>1.0.15</Version>
    <PackageId>MUNityBase</PackageId>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
  </ItemGroup>

</Project>
